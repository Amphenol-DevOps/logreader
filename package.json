{
  "scripts": {
    "build": "node ./node_modules/better-npm-run build",
    "lint": "eslint -c .eslintrc js",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "proxy": "node ./node_modules/better-npm-run proxy",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run proxy\""
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "proxy": {
      "command": "node webpack/dev-proxy.js",
      "env": {
        "NODE_PATH": "./js",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./js"
      }
    }
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "better-npm-run": "0.0.8",
    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-proxy": "^1.13.3",
    "json-loader": "0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-a11y": "0.3.3",
    "react-hot-loader": "1.3.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-plugin-typecheck": "3.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "oc-react-components": "^0.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-dropzone": "^3.5.0",
    "react-responsive": "1.1.3",
    "react-scrolla": "^0.3.0",
    "react-time": "^4.1.0"
  }
}
